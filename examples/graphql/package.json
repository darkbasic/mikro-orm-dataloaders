{
  "name": "graphql-example",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/mjs/index.d.ts",
        "default": "./dist/mjs/index.js"
      },
      "default": {
        "types": "./dist/mjs/index.d.ts",
        "default": "./dist/mjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "license": "MIT",
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm-config.ts",
      "./lib/mikro-orm-config.js"
    ]
  },
  "scripts": {
    "start": "NODE_ENV=development yarn run -T nodemon -w src -x ts-node-esm src/index.ts",
    "clean": "yarn run -T rimraf ./dist",
    "build": "yarn run compile",
    "compile": "yarn run compile:mjs && yarn run compile:cjs",
    "compile:mjs": "yarn run -T tsc --build --verbose",
    "compile:cjs": "yarn run -T tsc --build --verbose tsconfig.cjs.json",
    "test": "yarn run build"
  },
  "dependencies": {
    "@graphql-tools/executor-http": "^1.0.3",
    "@mikro-orm/core": "6.0.0-dev.200",
    "@mikro-orm/sqlite": "6.0.0-dev.200",
    "graphql": "16.8.1",
    "graphql-tag": "^2.12.6",
    "graphql-yoga": "5.0.0",
    "mikro-orm-find-dataloader": "^1.0.0"
  }
}
